{
    "name": "myvendor/ams",
    "description": "A simple PHP project",
    "require": {
        "php": ">=7.4",
        "milon/barcode": "^11.0",
        "picqer/php-barcode-generator": "^2.4",
        "endroid/qr-code": "^5.0",
        "khanamiryan/qrcode-detector-decoder": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "post-install-cmd": [
            "Composer\\Script\\Manager::ensureLocalRepository"
        ],
        "post-update-cmd": [
            "Composer\\Script\\Manager::ensureLocalRepository"
        ]
    }
}
